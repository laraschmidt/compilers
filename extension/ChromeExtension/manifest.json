{
    "name": "LEZ Extension",
    "version": "1.0",
    "description": "Making your first Google Chrome extension.",
    "manifest_version": 2,
    "options_page": "options.html",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*", "http://www.laraaschmidt.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_start"
    }
  ],
    "permissions": [
    "storage"
  ]
  
}
